{
  "name": "@alexski-perry/apus",
  "version": "0.8.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexski-perry/apus"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/alexski-perry"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "run": "ts-node -r tsconfig-paths/register example/example.ts",
    "test": "vitest run",
    "build": "tsup",
    "lint": "tsc --p ./tsconfig.json",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run build && changeset publish"
  },
  "dependencies": {
    "nanoid": "3",
    "neo4j-driver": "^5.23.0",
    "neo4j-driver-core": "^5.23.0",
    "ts-mixer": "^6.0.3",
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.0",
    "@changesets/cli": "^2.27.7",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "prettier": {
    "printWidth": 95,
    "arrowParens": "avoid"
  }
}
